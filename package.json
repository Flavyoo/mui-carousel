{
  "name": "@flavyoo/mui-carousel",
  "version": "0.0.1",
  "description": "A React and Material UI carousel component",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "test": "jest --config jestconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Flavyoo/typescript-starter-package.git"
  },
  "keywords": [
    "typescript",
    "typescript starter",
    "node starter",
    "npm package starter"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Flavyoo/typescript-starter-package/issues"
  },
  "homepage": "https://github.com/Flavyoo/typescript-starter-package#readme",
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "jest": "^27.4.5",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/material": "^5.2.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
